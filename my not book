# Removing coloumns 
import pandas as pd 
data  = {
    "Name":['Tashif','Noor','Adil','Aman'],
    "Age" : ['20','21','22','25'],
    "City" : ['india','pakistan','afirica','Nizambad'],
    "Salary":['1000000','20000','50000','50000']
}
df =pd.DataFrame(data)
print(df)

# df.drop(columns = ["Coloumns"],inplace=True)
df.drop(columns = ['Salary','Name','Age'], inplace=True)
print(df)

